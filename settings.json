{
  "editor.cursorSurroundingLines": 15,
  "editor.cursorSurroundingLinesStyle": "default",
  "editor.cursorBlinking": "expand",
  "editor.cursorStyle": "line",
  "workbench.list.smoothScrolling": true,
  "editor.formatOnSave": false,
  "editor.formatOnPaste": true,
  "terminal.integrated.smoothScrolling": true,
  "editor.occurrencesHighlight": "off",
  "editor.mouseWheelZoom": true,
  "editor.detectIndentation": true,
  "editor.fontLigatures": true,
  "editor.tabSize": 2,
  "git.autofetch": true,
  "editor.minimap.enabled": false,
  "search.searchOnType": false,
  "editor.lineHeight": 1.2,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.smoothScrolling": true,
  "explorer.compactFolders": false,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Halcyon",
  "editor.stickyScroll.enabled": false,
  "editor.wordWrap": "on",
  "workbench.activityBar.location": "top",
  "editor.guides.bracketPairs": true,
  "editor.guides.bracketPairsHorizontal": true,
  "window.customTitleBarVisibility": "auto",
  "extensions.autoUpdate": true,
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.fontFamily": "CaskaydiaMono Nerd Font",
  "editor.renderWhitespace": "none",
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.fontLigatures.enabled": true,
  "terminal.integrated.suggest.enabled": true,
  "editor.scrollBeyondLastLine": false,
  "editor.renderControlCharacters": false,
  "workbench.settings.enableNaturalLanguageSearch": false,
  "typescript.disableAutomaticTypeAcquisition": true,
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/node_modules": true
  },
  "search.followSymlinks": false,
  "files.autoSave": "onFocusChange",
  "workbench.editor.enablePreview": false,
  "window.menuBarVisibility": "toggle",

  "workbench.editorAssociations": {
    "*.copilotmd": "vscode.markdown.preview.editor"
  },
  "redhat.telemetry.enabled": false,
  "workbench.startupEditor": "none",
  "github.copilot.nextEditSuggestions.enabled": true,
  "github.copilot.advanced": {},
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "chat.agent.maxRequests": 500,
  "editor.unicodeHighlight.nonBasicASCII": false,
  "terminal.integrated.mouseWheelZoom": true,
  "chat.commandCenter.enabled": false,
  "dbcode.resultLocation": "panel",
  "dbcode.ai.inlineCompletion": false,
  "dbcode.ai.mcp.authorization": "None",
  "workbench.editor.empty.hint": "hidden",
  "chat.emptyChatState.enabled": false,
  "dbcode.connections": [],
  "dbcode.pagination": true,
  "chat.mcp.autostart": "never",
  "dbcode.paginationSize": "Auto",
  "window.zoomLevel": -0.25,
  "settingsSync.ignoredSettings": ["-window.zoomLevel"],
  "workbench.settings.openDefaultKeybindings": true,
  "workbench.settings.showAISearchToggle": false,
  "workbench.productIconTheme": "material-product-icons",
  "material-icon-theme.folders.color": "#8695b7",
  "material-icon-theme.folders.theme": "specific",
  "material-icon-theme.hidesExplorerArrows": true
}